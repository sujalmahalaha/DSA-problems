 left over suffix string                          if(st.find(pre)!=st.end() && (st.find(suf)!=st.end() || check(suf,st,mp)==true)) return mp[s]=true;         }         return mp[s]=false;     }          bool wordBreak(string s, vector<string> &dict) {         set<string> st;         for(int i=0;i<dict.size();i++) st.insert(dict[i]);                  map<string,bool> mp;         return check(s,st,mp);     } };
