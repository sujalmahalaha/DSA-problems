class Solution {
public:
    int search(vector<int>& nums, int target) {
        int low = 0;
         int high = nums.size()-1;
         while(low<=high){
             int mid = (high + low)/2;
             if (nums[mid]== target){
                 return mid ;
             }
             if (nums[low]<= nums[mid]){
                 // left sorted part
                 if(nums[low]<= target&& target<= nums[mid]){

                     high = mid -1;
                 }
                 else{
                     low = mid+1;
                 }
             }
             else{
                 // right part 
                 if(nums[mid ]<= target&& nums[high]>=target){
                     low = mid +1;

                 }
                 else{
                     high = mid -1;
                 }
             }
         }
         return -1;
        
    }
};
